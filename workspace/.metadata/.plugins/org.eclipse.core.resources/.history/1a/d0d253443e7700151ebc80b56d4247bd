package com.thahseen.javalistener;

import javax.servlet.http.HttpSessionEvent;
import javax.servlet.http.HttpSessionListener;

public class MyHttpSessionListener implements HttpSessionListener {

	@Override
	public void sessionCreated(HttpSessionEvent httpSessione) {
		  System.out.println("HttpSession is inintialized");
		
	}

	@Override
	public void sessionDestroyed(HttpSessionEvent httpSessione) {
		  System.out.println("HttpSession is destroyed");
		
	}

}
