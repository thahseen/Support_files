import java.util.Date;

public class MultiThread implements Runnable{

	public static void main(String[] args) {
		
      Thread t = new Thread();
      t.start();
	}

	@Override
	public void run() {
		Date d = new Date();
		for(int i=0;i<100;i++){
			System.out.println(t.currentThread().getId() + "is printing and the date is "+d.getTime() );
		}
		
		
	}

}
